CMD:regip(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    new ip[32];
	GetPlayerIp(playerid, ip, sizeof(ip));
    if(PlayerInfo[playerid][pAdmin] >= 9)
    {
		if(sscanf(params, "s",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "{4AC19D}[INFO] {FFFFFF} /regip [IP]");
		new regipdialog[256], Names[MAX_PLAYER_NAME];
		format(YCMDstr, sizeof(YCMDstr), "SELECT * FROM `accounts` WHERE pIpReg = '%s'", params[0]);
		mysql_query(connects,YCMDstr);
		new rows;
		cache_get_row_count(rows);
		if(!rows) return SendClientMessage(playerid,COLOR_GREY,"Am IP Adress it arcerti motamashe ar aris registrirebuli.");
		for new i = 0;i < rows; i++ do
		{
			cache_get_value_name(i,"Name",Names);
			format(YCMDstr, sizeof(YCMDstr), "{FFFFFF}%s\n",Names);
			strcat(regipdialog, YCMDstr);
		}
		ShowPlayerDialog(playerid, 7979, DIALOG_STYLE_TABLIST, "REGIP", regipdialog, "Close", "");
		format(log, sizeof(log), "[REGIP] Motamashe %s'm(a) Naxa IP: %s", Name(playerid),params[0]);
		CreateLog("REGIP.txt", log);
		return true;
	}
