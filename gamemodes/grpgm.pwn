stock MySQL_Connect()
{
    if(localhost == false)
	{
		connects = mysql_connect(mysql_host, mysql_user, mysql_pass, mysql_db);
	}
	if(localhost == true)
	{
		connects = mysql_connect(mysql_host, mysql_user, mysql_pass, mysql_db);
	}
	switch(mysql_errno())
	{
	    case 0: print("[G:RP] MySQL successfully loaded!");
	    default: print("[G:RP] MySQL unloaded!");
	}
 	mysql_log(ERROR | WARNING);
 	mysql_set_charset("cp1251");
    //
    mysql_query(connects, "UPDATE `accounts` SET `Online_status` = '1001'");
	mysql_tquery(connects,"SELECT * FROM `house`", "LoadProperty", "");
