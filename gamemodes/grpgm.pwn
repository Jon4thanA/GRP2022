CMD:buybiz(playerid)
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(PlayerInfo[playerid][pPbiskey] != 255) return errortxt(playerid, "Tkven Ukve Flobt Bizness!");
	{
		for(new b = 1; b <= TotalBizz; b++)
		{
			if(PlayerToPoint(5, playerid, BizzInfo[b][bEntranceX], BizzInfo[b][bEntranceY], BizzInfo[b][bEntranceZ]))
			{
			    if(BizzInfo[b][bType] != 4) return errortxt(playerid, "Am CMD's Gamoyeneba Shesadzlebelia Mxolod Kalonkaze!");
				if(BizzInfo[b][bOwned] == 1) return errortxt(playerid, "Es Biznesi Ukve Gayidulia!");
				if(PlayerInfo[playerid][pCash] < BizzInfo[b][bBuyPrice]) return errortxt(playerid, "Tkven Ar Gakvt Sakmarisi Tanxa!");
				new minstr[25];
				format(minstr, sizeof(minstr), "~r~-%d$", BizzInfo[b][bBuyPrice]);
				GameTextForPlayer(playerid, minstr, 1000, 1);
				PlayerInfo[playerid][pCash] -= BizzInfo[b][bBuyPrice];
				PlayerUpdateData(playerid, "pCash", PlayerInfo[playerid][pCash]);
				PlayerInfo[playerid][pPbiskey] = b;
				PlayerUpdateData(playerid, "pPbiskey", PlayerInfo[playerid][pPbiskey]);
				BizzInfo[b][bOwned] = 1;
				BizUpdateData(b, "bOwned", BizzInfo[b][bOwned]);
				strmid(BizzInfo[b][bOwner], Name(playerid), 0, strlen(Name(playerid)), 32);
				BizUpdateDataString(b, "bOwner", BizzInfo[b][bOwner]);
				BizzInfo[b][bTill] = 25000;
				BizUpdateData(b, "bTill", BizzInfo[b][bTill]);
				BizzInfo[b][bProducts] = 20000;
				BizUpdateData(b, "bProducts", BizzInfo[b][bProducts]);
    			if(BizzInfo[b][bType] != 4)
				{
					format(b_string, sizeof(b_string), "{FFFFFF}N: {FFFFFF}%d\n{647BD7}Biznesis Saxeli: {FFFFFF}%s\n{647BD7}Mflobeli: {FFFFFF}%s", b, BizzInfo[b][bMessage], BizzInfo[b][bOwner]);
				}
				else if(BizzInfo[b][bType] == 4)
				{
				    format(b_string, sizeof(b_string), "{FFFFFF}N: {FFFFFF}%d\n{647BD7}Biznesis Saxeli: {FFFFFF}%s\n{647BD7}Mflobeli: {FFFFFF}%s\n{FFFFFF}Sawvavis Fasi: {FFFFFF}%d$\n{FFFFFF}Sheiyvanet: {FFFFFF}/fill", b, BizzInfo[b][bMessage], BizzInfo[b][bOwner], BizzInfo[b][bPrice]);
				}
				Update3DTextLabelText(BizzInfo[b][bLabel], COLOR_WHITE, b_string);
			}
		}
	}
	return true;
}
