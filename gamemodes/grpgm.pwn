	SetPVarInt(playerid, "BBInt", GetPlayerInterior(playerid));
    RemovePlayerAttachedObject(playerid, 0);
	SetPVarInt(playerid, "BBVW", GetPlayerVirtualWorld(playerid));
	ApplyAnimation(playerid,"BOMBER","BOM_Plant",4.0,0,0,0,0,0);
