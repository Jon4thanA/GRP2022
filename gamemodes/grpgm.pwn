public OnPlayerSelectedMenuRow(playerid, row)
{
	if(GetPlayerMenu(playerid) == Admin)
	{
	    if(PlayerInfo[playerid][pAdmin] == 0 && PlayerInfo[playerid][pTecAdmin] == 0) return errortxt(playerid, "Tkven Ar Gakvt Am Funkciis Gamoyenebis Ufleba");
		new ClickedPlayerName[30]; GetPlayerName(SpecAd[playerid],ClickedPlayerName,24);
		switch(row)
		{
		case 0:StartSpectate(playerid,SpecAd[playerid]);
		case 1:
			{
				new mess[60];
				format(mess, sizeof(mess), "/check %d",SpecAd[playerid]);
				PC_EmulateCommand(playerid, mess);
				ShowMenuForPlayer(Admin,playerid);
			}
		case 2:
			{
				new mess[20];
				format(mess, sizeof(mess), "/gm %d",SpecAd[playerid]);
				PC_EmulateCommand(playerid, mess);
				ShowMenuForPlayer(Admin,playerid);
			}
		case 3:
			{
				if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING)
				{
				    ShowMenuForPlayer(Admin,playerid);
					StopSpectate(playerid);
					SetPlayerHealth(playerid, 100);
				}
			}
		}
	}
	return true;
}
