stock ConvertTimeAllT(time)
{
    new str[20];
    if(time > 60 && time < 3600)
    {
        new Float:TimeM,TimeS;
        TimeM = time / 60;
        TimeS = time % 60;
        format(str,sizeof(str),"%.0f:%02d",TimeM,TimeS);
    }
    else if(time < 60) format(str,sizeof(str),"0:%02d",time);
    return str;
}
