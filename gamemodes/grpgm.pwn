CMD:repcar(playerid)
{
	if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(PlayerInfo[playerid][pReptool] < 1) return SendClientMessage(playerid, COLOR_WHITE, "{FF0000}[Shecdoma] {FFFFFF}Tqven ar gaqvs xelsawyo manqanis shesaketeblad");
	if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "{FF0000}[Shecdoma] {FFFFFF}Manqanis shesaketeblad unda idget manqanastan");
	new nearveh = GetNearestVehicle(playerid);
	new carid;
	carid = nearveh;
	new Float:cx,Float:cy,Float:cz;
	GetVehiclePos(carid, cx, cy, cz);
	if(!PlayerToPoint(4.0, playerid, cx, cy, cz)) return SendClientMessage(playerid, COLOR_WHITE, "{FF0000}[Shecdoma] {FFFFFF}Tqven ar xart axlos arcert manqanastan");
	if(PlayerInfo[playerid][pReptool] >= 1)
	{
        VehicleRepair(carid);
		PlayerInfo[playerid][pReptool] -= 1;
		format(YCMDstr, 256, "{FF0000}[Repair] {FFFFFF}Tqven sheaketet manqana, darchenili xelsawyoebi: %d", PlayerInfo[playerid][pReptool]);
