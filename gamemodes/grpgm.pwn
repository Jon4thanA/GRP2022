	if(playerid == playerid)
	{
	    switch(PlayerInfo[playerid][pMember])
	    {
	        case 5:
	        {
	            if(POINT(playerid, 80.0, -50.1756,1400.3091,1084.4297))
	            {
	                if(GetPlayerVirtualWorld(playerid) != 2) return errortxt(playerid, "Iaragis Gaketeba Sheidzleba Tkvens Sakutar Bazaze");
				}
				else
				{
					errortxt(playerid, "Tkven Ar Xart Tkvens Bazaze");
					return true;
				}
	        }
	        case 6:
	        {
	            if(POINT(playerid, 80.0, -50.1756,1400.3091,1084.4297))
	            {
	                if(GetPlayerVirtualWorld(playerid) != 0) return errortxt(playerid, "Iaragis Gaketeba Sheidzleba Tkvens Sakutar Bazaze");
				}
				else
				{
					errortxt(playerid, "Tkven Ar Xart Tkvens Bazaze");
					return true;
				}
	        }
	        case 14:
	        {
	            if(POINT(playerid, 80.0, -50.1756,1400.3091,1084.4297))
	            {
	                if(GetPlayerVirtualWorld(playerid) != 1) return errortxt(playerid, "Iaragis Gaketeba Sheidzleba Tkvens Sakutar Bazaze");
				}
				else
				{
					errortxt(playerid, "Tkven Ar Xart Tkvens Bazaze");
					return true;
				}
