public OnPlayerEnterDynamicArea(playerid, areaid)
{
	for(new i = 1; i <= TotalRadar; i++)
	{
		if(areaid == RadarInfo[i][RadarArea])
		{
		    if(RadarInfo[i][RadarStatus] == 1)
		    {
   				if(GetPlayerState(playerid) == 2)
				{
					if(SpeedVehicle(playerid) > RadarInfo[i][RadarSpeed])
					{
						if(PlayerInfo[playerid][pCash] >= RadarInfo[i][RadarCash] && !IsaGovernment(playerid))
						{
						    PlayerInfo[playerid][pCash] -= RadarInfo[i][RadarCash];
						    PlayerUpdateData(playerid, "pCash", PlayerInfo[playerid][pCash]);
						    SendClientMessage(playerid, 0x2A7ADAAA, "{52CA9B}================================[ {FFFFFF}SPEED CAMERA{52CA9B} ]================================");
							format(b_string, sizeof(b_string), "[Radar N%d / Cash] {FFFFFF}Tkven Dajarimdit Sichkaris Gadacharbebistvis: {99FF00}%d${FFFFFF}'it", RadarInfo[i][RadarID], RadarInfo[i][RadarCash]);
							SendClientMessage(playerid, 0x2A7ADAAA, b_string);
							SendClientMessage(playerid,0x2A7ADAAA,"[INFO] {FFFFFF}Imistvis Rom Akontrolot Tkveni Sichkaret Da Agar Dajarimdet Gamoikenet CMD : /limit");
							SendClientMessage(playerid,0x2A7ADAAA,"[INFO] {FFFFFF}Tkven Chamogechrat Jarimis Safasuri Cash , Moufrtxildit Tkvens Da Sxvis Sicocxles");
							SendClientMessage(playerid, 0x2A7ADAAA, "{52CA9B}================================[ {FFFFFF}SPEED CAMERA{52CA9B} ]================================");
							format(l_string, sizeof(l_string), "~r~-%d$", RadarInfo[i][RadarCash]);
							GameTextForPlayer(playerid, l_string, 1000, 1);
							RadarMoney += RadarInfo[i][RadarCash];
						}
						else if(PlayerInfo[playerid][pCash] < RadarInfo[i][RadarCash] && PlayerInfo[playerid][pBank] >= RadarInfo[i][RadarCash] && !IsaGovernment(playerid))
						{
						    PlayerInfo[playerid][pBank] -= RadarInfo[i][RadarCash];
						    PlayerUpdateData(playerid, "pBank", PlayerInfo[playerid][pBank]);
                            SendClientMessage(playerid, 0x2A7ADAAA, "{52CA9B}================================[ {FFFFFF}SPEED CAMERA{52CA9B} ]================================");
							format(b_string, sizeof(b_string), "[Radar N%d / Bank] {FFFFFF}Tkven Dajarimdit Sichkaris Gadacharbebistvis: {99FF00}%d${FFFFFF}'it", RadarInfo[i][RadarID], RadarInfo[i][RadarCash]);
							SendClientMessage(playerid, 0x2A7ADAAA, b_string);
