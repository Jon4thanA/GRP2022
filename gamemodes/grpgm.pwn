CMD:arrest(playerid, params[])
{
	if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(FormaFrac[playerid] == 1 || FormaFrac[playerid] == 8 || FormaFrac[playerid] == 2 || FormaFrac[playerid] == 11 || FormaFrac[playerid] == 10 || FormaFrac[playerid] == 19 || PlayerInfo[playerid][pMember] == 11)
	{
		if(PlayerToPoint(10.0, playerid, 2413.7056,2481.3503,1563.6681))
		{
		    new prsid;
			if(sscanf(params, "u", prsid)) return SendClientMessage(playerid, COLOR_WHITE, "{4AC19D}[INFO] {FFFFFF} /arrest [Motamashis ID]");
			if(Knock_Status[prsid] == true) return infotxt(playerid, "Motamashe Danockilia");
	        if(PlayerInfo[prsid][pWantedLevel] < 1) return errortxt(playerid, "Motamashes Ar Adevs Arcerti Dzebnis Done!");
	    	if(prsid == INVALID_PLAYER_ID) return errortxt(playerid, "Motamashe Ver Moidzebna!");
			if(PlayerInfo[prsid][pLogin] != 1) return errortxt(playerid, "Motamashes Jer Ar Gauvlia Avtorizacia!");
			if(playerid == prsid) return errortxt(playerid, "Tkven Miutitet Tkveni ID!");
			if(!ProxDetectorS(2.0, playerid, prsid)) return errortxt(playerid, "Motamashe Ar Imyofeba Tkventan Axlos!");
			if(PlayerInfo[prsid][pJailed]) return infotxt(playerid, "Motamashe Ukve Imyofeba Cixeshi");
			new rand = random(sizeof(CixeSpawn));
			SetPlayerPos(prsid, CixeSpawn[rand][0], CixeSpawn[rand][1], CixeSpawn[rand][2]);
			SetPlayerFacingAngle(prsid, 84.8044);
	       	TogglePlayerControllable(playerid, 0);
            SetPlayerInterior(playerid, 1);
            SetPlayerVirtualWorld(playerid, 0);
			SetTimerEx("anfriz" , 3000, false, "d", playerid);
			PlayerInfo[prsid][pJailed] = 1;
			PlayerUpdateData(prsid, "pJailed", PlayerInfo[prsid][pJailed]);

			PlayerInfo[prsid][pJailTime] = PlayerInfo[prsid][pWantedLevel]*600;
			PlayerUpdateData(prsid, "pJailTime", PlayerInfo[prsid][pJailTime]);

			PlayerInfo[prsid][pWantedLevel] = 0;
			PlayerUpdateData(prsid, "pWantedLevel", PlayerInfo[prsid][pWantedLevel]);

			SetPlayerWantedLevel(prsid, 0);
			TogglePlayerControllable(prsid, 1);
			gotome[prsid] = -1;
			SetPlayerSpecialAction(prsid, SPECIAL_ACTION_NONE);
			RemovePlayerAttachedObject(prsid,0);
			PlayerCuffed[prsid] = 0;
			PlayerCuffedTime[prsid] = 0;

	        PlayerInfo[playerid][pPayCheck] += 100000;
		}
	}
	else
	{
	    errortxt(playerid, "Tkven Ar Gakvt Am CMD's Gamoyenebis Ufleba!");
	}
	return true;
}
