stock ShowItog(stolid)
{
	new maxs = 0,id = -1, cascount = 0;
  	foreach(new i:Player)
	{
   		if(GetPVarInt(i,"StolReg") == stolid && GetPVarInt(i,"Ochki") < 88)
   		{
     		new v = GetPVarInt(i,"Ochki");
     		if(v > maxs) { maxs = v; id = i; }
 		}
  	}
	foreach(new i:Player)
	{
	    if(GetPVarInt(i,"StolReg") == stolid && GetPVarInt(i,"Ochki") < 88 && i != id)
   		{
	    	new v = GetPVarInt(i,"Ochki");
	    	if(v == maxs) { cascount++; }
		}
	}
	if(cascount > 0)
	{
	    foreach(new i:Player)
		{
		    if(GetPVarInt(i,"StolReg") == stolid && GetPVarInt(i,"Ochki") < 88)
   			{
            	new v = GetPVarInt(i,"Ochki");
	    		if(v == maxs)
	    		{
   					SetPVarInt(i,"Ochki",88);
   					SetPVarInt(i,"StolReg",stolid);
   					TimeStavka[stolid] = 30;
				}
			}
		}
	}
	else
	{
  		new name[25];
	  	GetPlayerName(id,name,sizeof(name));
	  	new str[144];
	  	format(str,sizeof(str),"%s Igebs. Qulebis Odenoba: %d",name,maxs);
	  	new ammo = StolInfo[stolid][Bank]/90;
	  	PlayerInfo[StolInfo[stolid][Crupe]][pPayCheck]+=ammo/2;
	  	PlayerInfo[id][pCash]+=StolInfo[stolid][Bank]-ammo;
	  	PlayerUpdateData(id, "pCash", PlayerInfo[id][pCash]);
	  	if(StolInfo[stolid][Bank]-ammo >= 9500000)
	  	{
 			format(log, sizeof(log), "[CASINO WINNER] Motamashe %s'm(a) Moigo Tanxa: %d", name, StolInfo[stolid][Bank]-ammo);
			CreateLog("CasinoLog.txt", log);
	  	}
	  	if(stolid >= 0 && stolid <= 2)
	  	{
			if(KazinoInfo[1][Mafia] == 5) LCNBank+=ammo;
