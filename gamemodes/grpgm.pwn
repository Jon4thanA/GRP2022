CMD:fill(playerid)
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(!IsPlayerInAnyVehicle(playerid)) return errortxt(playerid, "Tkven Ar Imyofebit Transportshi!");
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return errortxt(playerid, "Tkven Ar Imyofebit Transportis Sachestan");
	if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 481 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 509 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 510) return errortxt(playerid, "Tkvens Transports Ar Schirdeba Sawvavi!");
    for(new b = 1; b <= TotalBizz; b++)
	{
		if(PlayerToPoint(10.0, playerid, BizzInfo[b][bEntranceX], BizzInfo[b][bEntranceY], BizzInfo[b][bEntranceZ]) && BizzInfo[b][bType] == 4 || PlayerToPoint(15.0,playerid,-1677.2297,409.2975,6.9068))
		{
			new Float:test = 200-Fuell[GetPlayerVehicleID(playerid)];
			if(BizzInfo[b][bProducts] <= 0 && BizzInfo[b][bOwned] == 1) return SendClientMessage(playerid,COLOR_GRAD1, "BenzinSadguri Ar Mushaobs"), BizzInfo[b][bProducts] = 0;
			//if(BizzInfo[b][bLocked] == 1 && BizzInfo[b][bOwned] == 1) return SendClientMessage(playerid,COLOR_GRAD1, "Sadguri Daxurulia");
			new car = GetPlayerVehicleID(playerid);
			if(Fuell[car] >= 50) return SendClientMessage(playerid, COLOR_GREY, "Tqvens Manqanashi Asxia Benzini");
			if(PlayerInfo[playerid][pCash] < BizzInfo[b][bPrice]) return SendClientMessage(playerid,COLOR_GRAD1, "Tqven Ar Gaqvt Amdeni Tanxa");
			if(!IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER || GetVehicleModel(car) == 481 || GetVehicleModel(car) == 509 || GetVehicleModel(car) == 510) return SendClientMessage(playerid,COLOR_YELLOW, "{E37070}[INFO]{ffffff}         .");
			BizzInfo[b][bTill] += BizzInfo[b][bPrice]/200*floatround(test)/10;
			PlayerInfo[playerid][pCash] -= BizzInfo[b][bPrice]/200*floatround(test)/10;
			if(BizzInfo[b][bOwned] == 1) BizzInfo[b][bProducts] -= 50;
			Fuell[car] = 100;
			if(GetPVarInt(playerid, "fixcarn") == 1) { PlayerInfo[playerid][pFuelcar] = 100; }
			if(GetPVarInt(playerid, "fixcarn") == 2) { PlayerInfo[playerid][pFuelcar1] = 100; }
			if(GetPVarInt(playerid, "fixcarn") == 3) { PlayerInfo[playerid][pFuelcar2] = 100; }
			if(GetPVarInt(playerid, "fixcarn") == 4) { PlayerInfo[playerid][pFuelcar3] = 100; }
			if(GetPVarInt(playerid, "fixcarn") == 5) { PlayerInfo[playerid][pFuelcar4] = 100; }
			format(YCMDstr,sizeof(YCMDstr),"Tqven Chaasxit Benzini. Sapasuri %d$",BizzInfo[b][bPrice]/200*floatround(test)/10);
			SendClientMessage(playerid,COLOR_BLUE,YCMDstr);
		}
	}
	return true;
}
CMD:recon(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(PlayerInfo[playerid][pTecAdmin] >= 1 || PlayerInfo[playerid][pAdmin] >= 1)
    {
		if(!IsPlayerConnected(playerid)) return true;
		if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /re(con) [ID Motamashis]");
		if(params[0] == playerid) return SendClientMessage(playerid, COLOR_GRAD1, "Tqven Tavs Ver Daakvirdebit");
		if(PlayerInfo[params[0]][pAdmin] > PlayerInfo[playerid][pAdmin]) return SendClientMessage(playerid,COLOR_GRAD1, "Administrators Ver Daakvirdebit");
		if(params[0] == INVALID_PLAYER_ID) return errortxt(playerid, "Motamashe Ver Moidzebna");
		if(!IsPlayerConnected(params[0])) return true;
		if(PlayerInfo[playerid][pFamilyText] != Text3D:INVALID_3DTEXT_ID)
		{
			DestroyDynamic3DTextLabel(PlayerInfo[playerid][pFamilyText]);
			PlayerInfo[playerid][pFamilyText] = Text3D:INVALID_3DTEXT_ID;
		}
		if(GetPlayerState(params[0]) == PLAYER_STATE_SPECTATING && gSpectateID[params[0]] != INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_GRAD1, "Motamashes Ukve Akvirdebian");
		if(GetPlayerState(params[0]) != 1 && GetPlayerState(params[0]) != 2 && GetPlayerState(params[0]) != 3) return SendClientMessage(playerid, COLOR_GRAD1, "Motamashe Ar Daspawnebula");
		StartSpectate(playerid, params[0]);
		if(!NonRecon(playerid))
		{
			new adminre[256];
   			format(adminre, sizeof(adminre), "{FFFF00}[A]{A4A4A4} %s  Akvirdeba  %s{ffffff}[%d]",Name(playerid),Name(params[0]),params[0]);
			SendAdminMessage(0xAFAFAFAA, adminre);
			SendTecAdmin(0xAFAFAFAA, adminre);
			SendCoderMessage(0xAFAFAFAA, adminre);
		}
	}
	return true;
}
