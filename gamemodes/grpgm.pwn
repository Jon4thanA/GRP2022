ForwardPublic UpdateSpeed(playerid)
{
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
	    new vehicleid = GetPlayerVehicleID(playerid), stext[30], Float:health;
        GetVehicleHealth(GetPlayerVehicleID(playerid), health);

		format(stext, sizeof(stext),"%d",SpeedVehicle(playerid));
		PlayerTextDrawSetString(playerid, VehicleSpeedDraw[playerid][18], stext);

		format(stext, sizeof(stext), "~g~%d ~w~HP", floatround(health));
  		PlayerTextDrawSetString(playerid, VehicleSpeedDraw[playerid][10], stext);

		if(Fuell[GetPlayerVehicleID(playerid)] <= 10)
  		format(stext, sizeof(stext), "~r~%.0f ~w~L", Fuell[GetPlayerVehicleID(playerid)] + 0.5);
		else format(stext, sizeof(stext), "~y~%.0f ~w~L", Fuell[GetPlayerVehicleID(playerid)] + 0.5);
		PlayerTextDrawSetString(playerid, VehicleSpeedDraw[playerid][14], stext);

  		format(stext, sizeof(stext), "%06d ~y~KM", VehicleKilometers[playerid]);
		PlayerTextDrawSetString(playerid, VehicleSpeedDraw[playerid][12], stext);

  	   	new Float:Mandzili[3];
		GetPlayerPos(playerid, Mandzili[0],Mandzili[1],Mandzili[2]);
		new GetCord = floatround(GetDistanceBetweenPoints(Cordinates[playerid][0],Cordinates[playerid][1],Cordinates[playerid][2], Mandzili[0],Mandzili[1],Mandzili[2]), floatround_round);
		if(GetCord > 150)
		{
		    VehicleKilometers[playerid]++;
			GetPlayerPos(playerid, Cordinates[playerid][0],Cordinates[playerid][1],Cordinates[playerid][2]);
		}
		new Float:X, Float:Y, Float:Z;
		GetVehicleVelocity(vehicleid,X,Y,Z);
		new Float:speed = floatsqroot(floatadd(floatadd(floatpower(X, 2), floatpower(Y, 2)),  floatpower(Z, 2))) * 100.3;

		if(speed >= SpeedLimit[playerid])
		{
			new Float:dif = SpeedLimit[playerid] / speed;
			SetVehicleVelocity(vehicleid,X*dif,Y*dif,Z);
			speed = SpeedLimit[playerid];
		}
	  	if(Light[GetPlayerVehicleID(playerid)] == 0)
		{
	        PlayerTextDrawHide(playerid, VehicleSpeedDraw[playerid][17]);
	        PlayerTextDrawColor(playerid, VehicleSpeedDraw[playerid][17], -1523963137);
	        PlayerTextDrawShow(playerid, VehicleSpeedDraw[playerid][17]);
		}
		if(Light[GetPlayerVehicleID(playerid)] == 1)
		{
			PlayerTextDrawHide(playerid, VehicleSpeedDraw[playerid][17]);
	        PlayerTextDrawColor(playerid, VehicleSpeedDraw[playerid][17], 780799999);
	        PlayerTextDrawShow(playerid, VehicleSpeedDraw[playerid][17]);
		}
  		if(zavodis[GetPlayerVehicleID(playerid)] == 0)
  		{
   			PlayerTextDrawHide(playerid, VehicleSpeedDraw[playerid][15]);
            PlayerTextDrawColor(playerid, VehicleSpeedDraw[playerid][15], -1523963137);
            PlayerTextDrawShow(playerid, VehicleSpeedDraw[playerid][15]);
        }
		if(zavodis[GetPlayerVehicleID(playerid)] == 1)
		{
   			PlayerTextDrawHide(playerid, VehicleSpeedDraw[playerid][15]);
            PlayerTextDrawColor(playerid, VehicleSpeedDraw[playerid][15], 780799999);
            PlayerTextDrawShow(playerid, VehicleSpeedDraw[playerid][15]);
        }
        if(IsLocked[GetPlayerVehicleID(playerid)] == 0)
		{
		    PlayerTextDrawHide(playerid, VehicleSpeedDraw[playerid][16]);
 			PlayerTextDrawColor(playerid, VehicleSpeedDraw[playerid][16], -1523963137);
 			PlayerTextDrawShow(playerid, VehicleSpeedDraw[playerid][16]);
        }
        if(IsLocked[GetPlayerVehicleID(playerid)] == 1)
		{
		    PlayerTextDrawHide(playerid, VehicleSpeedDraw[playerid][16]);
 			PlayerTextDrawColor(playerid, VehicleSpeedDraw[playerid][16], 780799999);
 			PlayerTextDrawShow(playerid, VehicleSpeedDraw[playerid][16]);
        }
		if(0 < SpeedVehicle(playerid) <= 200)
		{
			new check_color, Float:bar_speed;

			bar_speed = 525.999 + (0.436005 * SpeedVehicle(playerid));
			PlayerTextDrawTextSize(playerid, VehicleSpeedDraw[playerid][21], bar_speed, 9.457777);

			check_color = (SpeedVehicle(playerid) > 20) ? floatround(SpeedVehicle(playerid) / 20) : 0;
			if(check_color > (sizeof (speed_color) - 1)) check_color = sizeof (speed_color) - 1;
			PlayerTextDrawBoxColor(playerid, VehicleSpeedDraw[playerid][21], speed_color[check_color]);
			PlayerTextDrawShow(playerid, VehicleSpeedDraw[playerid][21]);
		}
	}
	else
	{
		if(speed_timer[playerid] != -1)
		{
			KillTimer(speed_timer[playerid]);
			speed_timer[playerid] = -1;
		}
		SpeedStatus[playerid] = false;
		for(new i; i != 22; i++) PlayerTextDrawHide(playerid, VehicleSpeedDraw[playerid][i]);
	}
	return true;
}
