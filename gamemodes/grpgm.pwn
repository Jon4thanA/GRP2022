CMD:uninvite(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(!IsPlayerConnected(playerid)) return true;
    if(sscanf(params, "us[250]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "{4AC19D}[INFO] {FFFFFF} /uninvite [ID Motamashis] [Mizezi]");
    if(PlayerInfo[playerid][pRank] <= PlayerInfo[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "Am Motamashes Ver Gaagdebt");
	if(POINT(playerid, 80.0, 1117.9392,-3.9645,1000.6797)) return SendClientMessage(playerid, COLOR_WHITE, "{FF0000}[Shecdoma] {FFFFFF}Cazinoshi Am CMD's Gamoyeneba Akrdzalulia!");
	if(params[0] == playerid) return SendClientMessage(playerid,-1,"Tqven Tavs Ver Gaagdebt");
    if(PlayerInfo[params[0]][pMember] == PlayerInfo[playerid][pLeader] || PlayerInfo[params[0]][pMember] == PlayerInfo[playerid][pMember])
    {
        if(!IsPlayerConnected(params[0])) return true;
        if(!CanUninvitePlayer(playerid,params[0])) return true;
        if(PlayerInfo[params[0]][pMember] <= 0) return true;
        format(YCMDstr, sizeof(YCMDstr), "%s'ma(m) Gagagdot Fraqciidan Mizezi: {FF0000}%s",Name(playerid),params[1]);
        SendClientMessage(params[0], 0x6BB3FFAA, YCMDstr);
        format(YCMDstr, sizeof(YCMDstr), "Tqven Gaagdet %s Fraqciidan Mizezi: {FF0000}%s",Name(params[0]),params[1]);
        SendClientMessage(playerid, 0x6BB3FFAA, YCMDstr);
        SendClientMessage(params[0], 0x6BB3FFAA, "Tqven Isev Moqalaqe Xart");
        SendClientMessage(params[0], 0x6BB3FFAA, "Samushao Dge Dasrulda");
        PlayerInfo[params[0]][pMember] = 0;
        Spawn_Type_Choosed[params[0]] = 1;
        PlayerInfo[params[0]][pLeader] = 0;
        PlayerInfo[params[0]][pRank] = 0;
        PlayerInfo[params[0]][pForma] = 0;
        SetPlayerArmourAC(params[0],0);
        ResetWeapons(params[0]);
        FormaFrac[params[0]] = 0;
        SetPlayerColor(params[0], TEAM_HIT_COLOR);
        SetPlayerSkin(params[0], PlayerInfo[params[0]][pChar][PlayerInfo[playerid][pViborChar]]);
        PlayerUpdateData(params[0], "pMember", PlayerInfo[params[0]][pMember]);
        PlayerUpdateData(params[0], "pLeader", PlayerInfo[params[0]][pLeader]);
        PlayerUpdateData(params[0], "pRank", PlayerInfo[params[0]][pRank]);
        PlayerUpdateData(params[0], "pForma", PlayerInfo[params[0]][pForma]);

    	PlayerInfo[playerid][pKills] = 0;
		PlayerUpdateData(playerid, "pKills", PlayerInfo[playerid][pKills]);
		PlayerInfo[playerid][pDeaths] = 0;
		PlayerUpdateData(playerid, "pDeaths", PlayerInfo[playerid][pDeaths]);

        dubinkon[params[0]] = false;
	    if(CaptureOn[playerid] == 1)
	    {
	    	CaptureOn[params[0]] = 0;
		    PlayerTextDrawHide(params[0], CaptureTextDraw_PTD[playerid][0]);
       		PlayerTextDrawHide(params[0], CaptureTextDraw_PTD[playerid][1]);
   			PlayerTextDrawHide(params[0], CaptureTextDraw_PTD[playerid][2]);
   			PlayerTextDrawHide(params[0], CaptureTextDraw_PTD[playerid][3]);
			KillCounter[params[0]] = 0;
		}
	}
    return true;
}
