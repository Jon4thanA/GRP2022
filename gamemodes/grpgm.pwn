CMD:admins(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 1)
    if(PlayerInfo[playerid][pTecAdmin] >= 1 || PlayerInfo[playerid][pAdmin] >= 1)
    {
		new Players;
		new adddmins;
	    SendClientMessage(playerid, COLOR_YELLOW, "Administrators Online:");
     	foreach(new i:Player)
		{
	        if(!IsPlayerConnected(i) || (!PlayerInfo[i][pLogin])) continue;
			Players++;
	        if(PlayerInfo[i][pAdmin] < 1) continue;
			if(PlayerInfo[i][pPnumber] == 418306) continue;
			if(PlayerInfo[i][pPnumber] == 941183) continue;
			if(PlayerInfo[i][pPnumber] == 886589) continue;
	        if(GetPVarInt(i,"AFK_Time") > 1)
	        {
		        format(YCMDstr, 256, "{81F79F}%s (ID: %d) [Admin Level: %d] - {FF8000}AFK: %d Wami",Name(i),i,PlayerInfo[i][pAdmin],GetPVarInt(i,"AFK_Time"));
		        SendClientMessage(playerid, COLOR_YELLOW2, YCMDstr);
		        adddmins++;
			}
			else
			{
				format(YCMDstr, 256, "{81F79F}%s (ID: %d) [Admin Level: %d]",Name(i),i,PlayerInfo[i][pAdmin],GetPVarInt(i,"AFK_Time"));
		        SendClientMessage(playerid, COLOR_YELLOW2, YCMDstr);
		        adddmins++;
			}
	    }
   		format(YCMDstr, 256, "Serverzea Sul %d Motamashe, Aqedan %d Administratori",Players,adddmins);
