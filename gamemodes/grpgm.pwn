public OnPlayerExitVehicle(playerid, vehicleid)
{
    SetPVarInt(playerid,"AFK_Time",0);
   // SetPVarInt(playerid, "GunCheckTime", 2);
    if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && TransportDuty[playerid] == 1)
    {
		TransportDuty[playerid] = 0;
		SendClientMessage(playerid, 0x6BB3FFAA, "{8099B5}Taxi Info {FFFFFF}Tqven Daasrulet Samushao Dge. Gisurvebt Warmatebebs");
		return true;
	}
	if(GodOFCarFix[playerid] == true)
	{
	    GodOFCarFix[playerid] = false;
	}
	if(FirstPersonCarStart[playerid] == true)
	{
		FirstPersonCarStart[playerid] = false;
		SetCameraBehindPlayer(playerid);
		DestroyPlayerObject(playerid,FirstPersonCarObject[playerid]);
	}
	if(gcontract[playerid] == 1)
	{
	    if(GetPlayerVehicleSeat(playerid) != 0) return true;
		Delete3DTextLabel(Meh3d[vehicleid]);
		gcontract[playerid] = 0;
		//SendClientMessage(playerid, -1, "Kontraqti Gauqmda!");
		return true;
	}
    if(GetPlayerState(playerid) == PLAYER_STATE_PASSENGER)
	{
		foreach(new i:Player)
		{
			if(IsPlayerInVehicle(i, GetPlayerVehicleID(playerid)) && GetPlayerState(i) == 2 && TransportDuty[i] == 1)
			{
				if(GetPVarInt(i,"Taxi_")-1 == playerid)
				{
					SendClientMessage(i, COLOR_BLUE, "{8099B5}Taxi Info {FFFFFF}Tkvenma Mgzavrma Datova Transporti, Tanxa Dagericxebat Payday'ze");
					if(GetPVarInt(i,"PayDayCashJob") > gettime()) {}
					else
					{
						PlayerInfo[i][pPayCheck] += 10000*PlayerInfo[i][ptaxilvl]*Boost2x;
      					new taxistr69[180];
						format(taxistr69, sizeof(taxistr69), "{8099B5}Taxi Info {FFFFFF}Tqvens Mier Shegrovebuli Tanxa Sheadgens: {8099B5}%d {FFFFFF}Romelic Dagericxebat PayDay-ze",PlayerInfo[i][pPayCheck]);
						SendClientMessage(i, COLOR_BLUE, taxistr69);
					}
					SetPVarInt(playerid, "TaxiPlayer", i);
					if(taxion[GetPVarInt(i,"Taxi_")-1] == false)
					{
						ShowPlayerDialog(GetPVarInt(i,"Taxi_")-1, 1203, DIALOG_STYLE_MSGBOX, "{8099B5}Glyfada {FFFFFF}Taxi", "{FFFFFF}Mogewonat Taxis Momsaxureba?", "KI", "ARA");
                        taxion[GetPVarInt(i,"Taxi_")-1] = true;
                        SetTimerEx("TaxiOFF", 60000, false, "i", GetPVarInt(i,"Taxi_")-1);
					}
					DeletePVar(i,"Taxi_");
					for(new id; id < GetMaxPlayers(); id++)
					{
						if(IsPlayerInVehicle(id, GetPlayerVehicleID(playerid)) && id != i && id != playerid)
						{
						    new taxistr[180];
							format(taxistr, sizeof(taxistr), "{8099B5}[Taxi Job] {FFFFFF}Miiyvanet Mgzavri: {8099B5}%s {FFFFFF}Danishnulebis Adgilas Da Xelfasi Payday'ze Dagericxebat",Name(id));
							SendClientMessage(i, COLOR_BLUE, taxistr);
							SetPVarInt(i,"Taxi_",id+1);
							break;
						}
					}
				}
			}
		}
	}
	new string[100];
	if(PEfir[playerid] != 255)
	{
		format(string, sizeof(string), "Tqven Gamoxvedit Pirdapiri Eteridan");
		SendClientMessage(playerid, COLOR_WHITE, string);
		PEfir[playerid] = 255;
	}
	if(Pefir[playerid] == 1 || Pefir[playerid] == 2)
	{
		PEfir[playerid] = 255;
		format(string, sizeof(string), "Tqven Gamoxvedit Pirdapiri Eteridan");
		SendClientMessage(playerid, COLOR_WHITE, string);
	}
	return true;
}
public OnPlayerRequestClass(playerid, classid)
{
    if(IsPlayerNPC(playerid)) return true;
	new pup[16];
	GetPlayerIp(playerid,pup,sizeof(pup));
	/* -------------------------------------------- DELETE FROM `banip` WHERE */
	new hgh[156];
	format(hgh, sizeof(hgh), "SELECT * FROM `banip` WHERE IP = '%s'",pup);
	mysql_tquery(connects, hgh, "CheckBanIP", "i",playerid);
	/* ---------------------------------------------------------------------- */
    if(PlayerInfo[playerid][pLogin] == 1) return SpawnPlayer(playerid);
    PlayerInfo[playerid][pCash] = 0;
    GivePlayerMoney(playerid, -GetPlayerMoney(playerid));
    ResetNew(playerid);
	SetPlayerInterior(playerid, 0);
 	InterpolateCameraPos(playerid, 1904.933959, -683.142456, 145.723770, 1370.436889, -1461.484008, 240.051498, 20000);
	InterpolateCameraLookAt(playerid, 1901.398315, -686.653625, 145.310180, 1373.690063, -1458.373413, 237.874237, 20000);
	SetPlayerPos(playerid,358.2432,-2034.5244,7.8359);
	TogglePlayerControllable(playerid,0);
	format(bac, sizeof(bac), "SELECT `Name` FROM `accounts` WHERE name = '%s'",Name(playerid));
	mysql_query(connects,bac);
	new rows;
	cache_get_row_count(rows);
	if(rows)
	{
        Authorization(playerid);
	}
	else
	{
		Registration(playerid);
    }
	return true;
}
