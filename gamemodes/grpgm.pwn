CMD:lock(playerid)
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    new carid;
	if(PlayerInfo[playerid][pPhousekey] == 999 && PlayerInfo[playerid][pQira] == 999 && PlayerInfo[playerid][pCarHouse] == 0 && caridhouse[playerid] == -1) return true;
	if(caridhouse[playerid] != -1) carid = caridhouse[playerid];
	new lockstatus = gCarLock[carid];
	new Float:cx,Float:cy,Float:cz;
	GetVehiclePos(carid, cx, cy, cz);
	if(!PlayerToPoint(4.0, playerid, cx, cy, cz)) return true;
	switch (lockstatus)
	{
		case 0:
		{
			GameTextForPlayer(playerid, "~w~CAR ~r~LOCK", 3000, 6);
			PlayerPlaySound(playerid, 24600, 0.0, 0.0, 0.0);
			LockCar(carid);
			IsLocked[caridhouse[playerid]] = 1;
			new enginestr[256];
			format(enginestr, sizeof(enginestr), "%s'm(a) Chaketa Manqanis Karebi", Name(playerid));
			ProxDetector(20.0, playerid, enginestr, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
		}
		case 1:
		{
			GameTextForPlayer(playerid, "~w~CAR ~g~UNLOCK", 3000, 6);
			PlayerPlaySound(playerid, 24600, 0.0, 0.0, 0.0);
			UnLockCar(carid);
			IsLocked[caridhouse[playerid]] = 0;
			new enginestr[256];
			format(enginestr, sizeof(enginestr), "%s'm(a) Gaago Manqanis Karebi", Name(playerid));
			ProxDetector(20.0, playerid, enginestr, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
